{
    "name": "kraenkvisuell/nova-tabs",
    "description": "Laravel Nova - Tabs",
    "keywords": [
        "laravel",
        "nova"
    ],
    "authors": [
        {
            "name": "Emini Arts GmbH",
            "email": "support@eminiarts.ch",
            "homepage": "https://eminiarts.ch"
        },
        {
            "name": "Krank Visuell GbR",
            "email": "kontakt@kraenk.de",
            "homepage": "https://kraenk.de"
        }
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": ">=7.3.0"
    },
    "require-dev": {
        "laravel/nova": "~3.0",
        "orchestra/testbench": "^5.3",
        "phpunit/phpunit": "^9.2"
    },
    "autoload": {
        "psr-4": {
            "Kraenkvisuell\\Tabs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kraenkvisuell\\Tabs\\TabsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
